<div class="py-8">
  <p class="font-extrabold text-6xl">
    Logged in as <%= current_user.name %><br><br>
    Your Feed:<br>
    <% @posts.each do |p| %>
      <% if p.display_post?(current_user) %>
        <%= p.user.name %><br>
        <%= p.body %><br>
        <%= p.created_at %><br><br>
      <% end %>
    <% end %>
    <%= link_to 'Back to User Index', users_index_path %><br>
    <%= link_to 'Sign out', destroy_user_session_path, method: :delete %>
  </p>
</div>
